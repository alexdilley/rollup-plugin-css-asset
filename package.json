{
  "name": "rollup-plugin-css-asset",
  "version": "1.0.1",
  "description": "Bundle CSS as a Rollup asset",
  "main": "index.js",
  "scripts": {
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js .",
    "lint:all:markdownlint": "yarn lint:markdownlint '**/*.md'",
    "lint:all:prettier": "yarn lint:prettier '**/*.{css,html,md,js}'",
    "lint": "run-s lint:all:*",
    "test": "tape test/*.js | tap-spec"
  },
  "repository": "https://github.com/alexdilley/rollup-plugin-css",
  "keywords": [
    "rollup-plugin",
    "css"
  ],
  "author": "Alex Dilley <alex.dilley@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.8.0",
    "markdownlint-cli": "^0.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.0"
  },
  "peerDependencies": {
    "rollup": ">=1.19.0"
  },
  "dependencies": {
    "clean-css": "^4.2.3",
    "concat-with-sourcemaps": "^1.1.0",
    "convert-source-map": "^1.7.0",
    "rollup-pluginutils": "^2.8.2"
  }
}
